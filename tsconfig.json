{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@services/*": ["./src/services/*"],
      "@utils/*": ["./src/utils/*"],
      "@styles/*": ["./src/styles/*"],
      "@views/*": ["./src/views/*"],
      "@router/*": ["./src/router/*"],
      "@react/*": ["./src/react/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
